<p style="color: green"><%= notice %></p>

<% content_for :title, "Prompts" %>

<h1>Prompts</h1>

<div id="prompts">
  <% @prompts.each do |prompt| %>
    <%= render prompt %>
    <p>
      <%= link_to "Show this prompt", prompt %>
    </p>
  <% end %>
</div>

<%= link_to "New prompt", new_prompt_path %>
